D,CAA2D,UAAC,QAAD,EAAc;AACvE,YAAG,QAAQ,CAAC,MAAT,KAAkB,GAArB,EAAyB;AACvB,UAAA,KAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,OAAlB;AACF;AACD,OAJD,EAIE,UAAC,KAAD,EAAS;AACT,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAND;AAQF;AAvCM,GAbK;AAsDb,EAAA,OAtDa,qBAsDH;AAAA;;AACR,SAAK,EAAL,GAAU,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAA7B;AACA,SAAK,OAAL,GAAe,KAAK,QAAL,CAAc,IAAd,CAAmB,UAAA,OAAM;AAAA,aAAK,OAAO,CAAC,EAAR,IAAc,MAAI,CAAC,EAAxB;AAAA,KAAzB,CAAf;AACA,SAAK,QAAL,GAAgB,KAAK,UAAL,CAAgB,IAAhB,CAAqB,UAAA,QAAO;AAAA,aAAK,QAAQ,CAAC,EAAT,IAAe,MAAI,CAAC,OAAL,CAAa,UAAjC;AAAA,KAA5B,CAAhB;AACA,SAAK,KAAL,GAAa,YAAY,CAAC,OAAb,CAAqB,OAArB,CAAb;AACF;AA3Da,CAAf","sourcesContent":["<template>\n    <div class=\"container\">\n      <div class=\"row pt-5\">\n        <div class=\"col-md-1\">\n        </div>\n        <div class=\"col-md-4 col-12\">\n          <img :src=\"product.imageURL\" :alt=\"product.name\" class=\"img-fluid\">\n        </div>\n        <div class=\"col-md-6 col-12 pt-3 pt-md-0\">\n          <h4>{{product.name}}</h4>\n          <h6 class=\"category font-italic\">{{category.categoryName}}</h6>\n          <p><span class=\"font-weight-bold\">Description: -</span> <br>{{product.description}}</p>\n\n          <div class=\"d-flex flex-row justify-content-between\">\n            <div class=\"input-group col-md-3 col-4 p-0\">\n              <div class=\"input-group-prepend\">\n                <span class=\"input-group-text\" id=\"basic-addon1\">Quantity</span>\n              </div>\n              <input class=\"form-control\" type=\"number\" v-bind:value=\"quantity\"/>\n            </div>\n\n            <div class=\"input-group col-md-3 col-4 p-0\">\n              <button type=\"button\" id=\"add-to-cart-button\" class=\"btn\" @click=\"addToCart(this.id)\">\n                Add to Cart\n                <ion-icon name=\"cart-outline\" v-pre></ion-icon>\n              </button>\n            </div>\n          </div>\n\n          <div class=\"features pt-3\">\n            <h5><strong>Features</strong></h5>\n            <ul>\n              <li>Lorem ipsum dolor sit amet consectetur adipisicing elit.</li>\n              <li>Officia quas, officiis eius magni error magnam voluptatem</li>\n              <li>nesciunt quod! Earum voluptatibus quaerat dolorem doloribus</li>\n              <li>molestias ipsum ab, ipsa consectetur laboriosam soluta et</li>\n              <li>ut doloremque dolore corrupti, architecto iusto beatae.</li>\n            </ul>\n          </div>\n\n            <!-- <button id=\"wishlist-button\" class=\"btn mr-3 p-1 py-0\" :class=\"{product_added_wishlist: isAddedToWishlist}\" @click=\"addToWishList(this.id)\">\n              {{wishlistString}}\n            </button> -->\n            <button id=\"show-cart-button\" type=\"button\" class=\"btn mr-3 p-1 py-0\" @click=\"listCartItems()\">\n              Show Cart\n              <ion-icon name=\"cart-outline\" v-pre></ion-icon>\n            </button>\n\n        </div>\n        <div class=\"col-md-1\">\n\n        </div>\n\n      </div>\n\n    </div>\n\n</template>\n\n<script>\nexport default {\n  data(){\n    return {\n      product : {},\n      category : {},\n      id : null,\n      token: null,\n      // isAddedToWishlist: false,\n      // wishlistString:\"Add to wishlist\",\n      quantity: 1\n    }\n  },\n  props : [\"baseURL\",\"products\", \"categories\"],\n  methods:{\n    // addToWishList(productId){\n    //         axios.post(`${this.baseURL}wishlist/add?token=${this.token}`, {\n    //             id:productId\n    //         }).then((response) => {\n    //             if(response.status==201) {\n    //                 this.isAddedToWishlist = true;\n    //                 this.wishlistString = \"Added to WishList\"\n    //             }\n    //         },(error) =>{\n    //             console.log(error)\n    //         });\n    //     },\n    addToCart(productId){\n      axios.post(`${this.baseURL}cart/add?token=${this.token}`,{\n          productId : productId,\n          quantity : this.quantity\n      }).then((response) => {\n        if(response.status==201){\n          swal({\n            text: \"Product Added to the cart!\",\n            icon: \"success\",\n            closeOnClickOutside: false,\n          });\n        }\n      },(error) =>{\n        console.log(error)\n      });\n    },\n\n    listCartItems(){\n      axios.get(`${this.baseURL}cart/?token=${this.token}`).then((response) => {\n        if(response.status===200){\n          this.$router.push('/cart')\n        }\n      },(error)=>{\n        console.log(error)\n      });\n\n    }\n  },\n  mounted() {\n    this.id = this.$route.params.id;\n    this.product = this.products.find(product => product.id == this.id);\n    this.category = this.categories.find(category => category.id == this.product.categoryId);\n    this.token = localStorage.getItem('token');\n  }\n}\n</script>\n\n<style>\n.category {\n  font-weight: 400;\n}\n\n/* Chrome, Safari, Edge, Opera */\ninput::-webkit-outer-spin-button,\ninput::-webkit-inner-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\n\n/* Firefox */\ninput[type=number] {\n  -moz-appearance: textfield;\n}\n\n#add-to-cart-button {\n  background-color: #febd69;\n}\n\n#wishlist-button {\n  background-color: #b9b9b9;\n  border-radius: 0;\n}\n\n#show-cart-button {\n  background-color: #131921;\n  color: white;\n  border-radius: 0;\n}\n</style>\n"],"sourceRoot":""}]}